{
  "name": "dotlottie-solid",
  "version": "1.1.1",
  "description": "An unofficial Solid library for rendering lottie and dotLottie animations in the browser.",
  "license": "MIT",
  "author": "moonlitgrace",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonlitgrace/dotlottie-solid.git"
  },
  "homepage": "https://github.com/moonlitgrace/dotlottie-solid#readme",
  "bugs": {
    "url": "https://github.com/moonlitgrace/dotlottie-solid/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 \"src/**/*.{js,ts,tsx,jsx}\"",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.8.17"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "@types/node": "^20.13.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.21.4",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "jsdom": "^24.1.0",
    "prettier": "3.2.5",
    "solid-js": "^1.8.17",
    "tsup": "^8.0.2",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "solid",
    "dotlottie",
    "lottie",
    "player",
    "animation",
    "web",
    "canvas",
    "javascript",
    "thorvg"
  ],
  "packageManager": "pnpm@9.1.3",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-web": "^0.23.1",
    "debounce": "^2.1.0"
  }
}
